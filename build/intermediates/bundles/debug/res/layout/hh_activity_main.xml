<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/hh_rl_base_parent"
    style="@style/hh_fi_fi">

    <LinearLayout
        android:id="@+id/hh_ll_base_bottom"
        style="@style/hh_fi_wr"
        android:layout_alignParentBottom="true"
        android:orientation="vertical"></LinearLayout>

    <LinearLayout
        android:id="@+id/hh_ll_base_top"
        style="@style/hh_fi_wr"
        android:orientation="vertical"></LinearLayout>

    <FrameLayout
        android:id="@+id/hh_fl_base_container"
        style="@style/hh_fi_fi"
        android:layout_above="@id/hh_ll_base_bottom"
        android:layout_below="@id/hh_ll_base_top"></FrameLayout>


</RelativeLayout>
